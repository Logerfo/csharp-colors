{
    "name": "csharp-colors",
    "displayName": "C# Colors",
    "description": "Highlight C# colors in your editor",
    "version": "0.0.8",
    "publisher": "logerfo",
    "keywords": [
        "c#",
        "csharp",
        "color"
    ],
    "engines": {
        "vscode": "^1.10.0"
    },
    "bugs": {
        "url": "https://github.com/Logerfo/csharp-colors/issues",
        "email": "bruno@logerfo.tk"
    },
    "author": {
        "email": "bruno@logerfo.tk",
        "name": "Bruno Logerfo",
        "url": "https://github.com/Logerfo"
    },
    "galleryBanner": {
        "color": "#181818",
        "theme": "dark"
    },
    "icon": "icon.png",
    "license": "Unlicense",
    "homepage": "https://github.com/Logerfo/csharp-colors/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/Logerfo/csharp-colors.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "csharp-colors.colorHighlight",
                "title": "Highlight colors in current file"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^3.0.3",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "color": "^3.0.0",
        "color-name": "^1.1.4"
    }
}
