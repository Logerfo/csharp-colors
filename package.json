{
    "name": "csharp-colors",
    "displayName": "C# Colors",
    "description": "Highlight C# colors in your editor",
    "version": "0.2.1",
    "publisher": "logerfo",
    "keywords": [
        "c#",
        "csharp",
        "color"
    ],
    "engines": {
        "vscode": "^2023.0.0"
    },
    "bugs": {
        "url": "https://github.com/Logerfo/csharp-colors/issues",
        "email": "bruno@logerfo.com.br"
    },
    "author": {
        "email": "bruno@logerfo.com.br",
        "name": "Bruno Logerfo",
        "url": "https://github.com/Logerfo"
    },
    "galleryBanner": {
        "color": "#181818",
        "theme": "dark"
    },
    "icon": "icon.png",
    "license": "Unlicense",
    "homepage": "https://github.com/Logerfo/csharp-colors/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/Logerfo/csharp-colors.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:csharp"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "csharp-colors.colorHighlight",
                "title": "Highlight colors in current file"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "csharp-colors.colorHighlight",
                    "when": "false"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "webpack --mode production",
        "compile": "webpack --mode none",
        "watch": "webpack --mode none --watch"
    },
    "devDependencies": {
        "@types/mocha": "10.0.10",
        "@types/node": "22.10.5",
        "ts-loader": "9.5.1",
        "tslint": "6.1.3",
        "typescript": "5.7.3",
        "@types/vscode": "1.14.0",
        "webpack": "5.97.1",
        "webpack-cli": "6.0.1"
    },
    "dependencies": {
        "color": "^4.0.0",
        "color-name": "^2.0.0"
    }
}
